{
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "markdown-it-sub": [
        "./src/types/markdown-it-sub.d.ts"
      ],
      "markdown-it-sup": [
        "./src/types/markdown-it-sub.d.ts"
      ],
      "markdown-it-for-inline": [
        "./src/types/markdown-it-for-inline.d.ts"
      ]
    }
  }
}
